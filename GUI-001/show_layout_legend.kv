<Blabel@Label>
    markup: True
    font_size: (root.height*1**2 + root.width*1**2)/7
    halign: 'center'
    #valign: 'middle'
	background_color: (0,0,0,0)
	#background_normal: ''
	color: (0,0,0,1) #text_color
	size_hint: (0.030927835, 0.055)  #600/1067=0,562324274

	canvas.before:
		Color:
			rgba: (.08,.08,.08,1)
		RoundedRectangle:
			size: self.size
			pos: self.pos
			radius: [100]